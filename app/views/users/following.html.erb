<h2>Follow User</h2>
<% if @users.empty? %>
<h1>ユーザーはいません</h1>
<% else %>
<table class='table'>
  <thead>
    <tr>
      <th>image</th>
      <th>name</th>
      <th></th>
      <th></th>
    </tr>
  </thead>
  <tbody>
   <% @user.following_user.where.not(id: @user.id).each do |user| %>
   <tr>
     <td><%= attachment_image_tag(user, :profile_image, :fill, 100, 100, fallback: "no_image.jpg") %></td>
     <td><%=  user.name %></td>
     <td><%= "フォロー数: #{user.follower.count}" %></td>
     <td><%= "フォロワー数: #{user.followed.count}" %></td>
     <td><%= link_to 'Show', user_path(user), class: "user_#{user.id}" %></td>
   </tr>
   <% end %>
  </tbody>
 </table>
 <% end %>
  





